{{!-- <!-Búsqueda Usuario> --}}
<section class ="searchUser-general">
  <form class="d-flex form-home" method="GET" action="/searchUser">
    <input
     class="form-control me-2"
     type="text"
     id="searchUser"
     name="searchUser"
     placeholder="Search" 
     aria-label="Search"
    />
   <button class="btn btn-light-navhome" type="submit">Search Users</button>
  </form>
</section>

{{!-- <!-Menú Lateral Top 10> --}}
<aside class="aside-general">
  <div class="containerArtists">
    <div class = "row" >
      <div class = "col-md-4">
        <div class = "right-aligned-container-home">
          <h3 class="header-top"><strong><u> BeatBreeze Top 10</u></strong></h3>
          {{#each charts as |chart|}}
            {{> trackPlayer info=chart}}
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </div>
</aside>
{{!-- <!-Energized> --}}
<h1 class ="encabezado"><strong><u>Energized</u></strong></h1>
<div class="containerArtists">
  <div class="row">
  {{#each strongest as |strong|}}
      <div class="col-md-4">
        <div class="container-recommendations">
        {{> trackPlayer info=strong}}
          <div class="card-body">
            <h5 class="card-title"></h5>
          </div>
        </div>
      </div>
  {{/each}} 
